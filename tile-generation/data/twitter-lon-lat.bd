# web-mercator (or spherical-mercator) projection
oculus.binning.projection=EPSG:900913
# The field to use as the X axis in all tiles
oculus.binning.xField=longitude
# The field to use as the Y axis in all tiles
oculus.binning.yField=latitude
# The levels to tile.  These values indicate that the system should tile
# levels 0-3 in one pass, then level 4 in the second pass, then level 5
# in the third pass, etc.
oculus.binning.levels.0=0,1,2,3
oculus.binning.levels.1=4
oculus.binning.levels.2=5
oculus.binning.levels.3=6
oculus.binning.levels.4=7
oculus.binning.levels.5=8
oculus.binning.levels.6=9
