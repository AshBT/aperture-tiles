{
	"annotations-unit-test": {
        "id": "annotations-unit-test",
        "pyramid" : {
            "type" : "WebMercator"
        },
        "data": {
			"pyramidio": {
				"type": "file",
				"root.path": ".\\",
				"extension": "json"
			},
			"serializer": {
				"type": "string->[(string, long)]-j"
			}
		},
		"groups": [ "Urgent", "High", "Medium", "Low" ],
		"filter" : {
			"type": "n-most-recent-by-group",
			"countsByGroup": {
				"Urgent": 100,
				"High": 100,
				"Medium": 100,
				"Low" : 100
			}
		}
    }
}
